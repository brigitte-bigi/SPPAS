msgid ":ERROR 9999: "
msgstr "Version dépréciée de Python: SPPAS requiert au moins la version 3.7+. "

msgid ":ERROR 9990: "
msgstr "Version dépréciée de Python: la fonctionnalité {} de SPPAS requiert au moins la version {}."

msgid ":ERROR 0001: "
msgstr "L'erreur suivante a été relevée: {message}."

msgid ":ERROR 0100: "
msgstr "{!s:s} n'est pas du type attendu: '{}'."

msgid ":ERROR 0200: "
msgstr "Valeur d'index invalide: {:d}."

msgid ":ERROR 0300: "
msgstr "La valeur '{!s:s}' est incorrecte pour '{!s:s}'."

msgid ":ERROR 0310: "
msgstr "Une valeur positive était attendue, alors que {value} a été donnée."

msgid ":ERROR 0320: "
msgstr "La valeur minimale donnée {min_value} est supérieur à la valeur maximale donnée {max_value}."

msgid ":ERROR 0330: "
msgstr "La valeur {value} est en dehors de l'intervalle [{min_value},{max_value}]."

msgid ":ERROR 0340: "
msgstr "L'index {value} est en dehors de l'intervalle [{min_value},{max_value}]."

msgid ":ERROR 0350: "
msgstr "Code de langue '{code}' invalide."

msgid ":ERROR 0400: "
msgstr "Clé inconnue '{!s:s}' pour les données '{!s:s}'"

msgid ":ERROR 0500: "
msgstr "Erreur système: {error}"

msgid ":ERROR 0505: "
msgstr "Le système {} n'est pas supporté actuellement. Aucun installeur automatique n'a pu être trouvé."

msgid ":ERROR 0510: "
msgstr "L'installation a induit des erreurs et/ou warnings: {error}."

msgid ":ERROR 0513: "
msgstr "Erreur de permission: l'accès à '{place}' a été refusé."

msgid ":ERROR 0520: "
msgstr "La fonctionnalité {name} n'est pas activée; son installation est requise avant de pouvoir l'utiliser."

msgid ":ERROR 0530: "
msgstr "Le package {package} n'a pas pu être importé. L'installation de la fonctionnalité {name} est requise pour pouvoir l'utiliser."

msgid ":ERROR 0540: "
msgstr "Le package {package} nécessite une mise à jour. Une ré-installation de la fonctionnalité {name} est requise pour pouvoir l'utiliser."

msgid ":ERROR 0610: "
msgstr "L'extension du fichier {:s} est inconnue"

msgid ":ERROR 0620: "
msgstr "Le dossier {dirname} n'existe pas."

msgid ":ERROR 0650: "
msgstr "Echec lors de l'ouverture ou de la lecture du fichier '{:s}'."

msgid ":ERROR 0655: "
msgstr "Echec lors de la lecture du fichier {name}: {msg}"

msgid ":ERROR 0660: "
msgstr "Echec lors de la sauvegarde du fichier '{:s}'."

msgid ":ERROR 0670: "
msgstr "Les fichiers avec l'extension '{:s}' ne peuvent pas être ouverts."

msgid ":ERROR 0680: "
msgstr "Les fichiers avec l'extension '{:s}' ne peuvent pas être enregistrés."

msgid ":INFO 0510: "
msgstr "Installation de '{name}'"

msgid ":INFO 0520: "
msgstr "'{name}' n'est pas disponible pour votre système d'exploitation."

msgid ":INFO 0530: "
msgstr "L'installation de '{name}' est désactivée."

msgid ":INFO 0540: "
msgstr "'{name}' est installé."

msgid ":INFO 0550: "
msgstr "L'installation de '{name}' a échouée."

msgid ":INFO 0560: "
msgstr "Installation terminée"

msgid ":INFO 570: "
msgstr "'{name}' ne correspond à aucun package valide."

msgid ":INFO 0580: "
msgstr "Aucune commande système n'est nécessaire."

msgid ":ERROR 0977: "
msgstr "Une donnée requise '{:s}' n'a pas été saisie."
